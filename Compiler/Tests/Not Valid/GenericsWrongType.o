class A[T] is
    this(t: T)
        var b: t
    end
end

class B[TKey, TValue] is
    this(k: TKey, v: TValue)
        var key: k
        var value: v
    end
end

class C[TOne, TTwo, TThree] is
    this(k: TOne, v: TTwo, w: TThree)
        var one: k
        var two: v
        var three: w
    end
end

class D is
    this is
        var a: A[](4) // Missed type specification
        var b: B[TKey, TValue](1, true) 
        var c: C[COne, CTwo, CThree](5.4, false, 5)  //не передается реальный тип
    end
end

